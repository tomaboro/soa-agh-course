<serviceGroup>
    <service name="HelloWorld">
        <parameter name="ServiceClass" locked="false">example.HelloWorld</parameter>
        <operation name="*">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
        </operation>
    </service>
    <service name="HolidaysCountDown">
        <parameter name="ServiceClass" locked="false">com.motek.soa.HolidaysCountDown</parameter>
        <operation name="*">
            <messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
        </operation>
    </service>
</serviceGroup>
